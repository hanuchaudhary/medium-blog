
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @unique @default(uuid())
  email String @unique
  name String
  blog Blog[]
  password String
}

model Blog {
  id String @unique @default(uuid())
  title String
  content String
  published Boolean @default(false)
  authorId String
  author User @relation(fields: [authorId], references: [id])
}
